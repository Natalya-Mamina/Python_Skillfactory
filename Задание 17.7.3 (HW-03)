per_cent = { 'ТКБ' : 5.6, 'СКБ' : 5.9, 'ВТБ' : 4.28, 'СБЕР' : 4.0 }
деньги = int(input("Введите сумму депозита: "))
per_cent_ = list(per_cent.values())

money = []
money.extend([деньги])


ТКБ_ = per_cent_[0]*money[0]
СКБ_ = per_cent_[1]*money[0]
ВТБ_ = per_cent_[2]*money[0]
СБЕР_ = per_cent_[3]*money[0]
депозит = []
депозит.extend([ТКБ_, СКБ_, ВТБ_, СБЕР_])
print(депозит)

print('Максимальная сумма, которую вы можете заработать — ', max(депозит))

