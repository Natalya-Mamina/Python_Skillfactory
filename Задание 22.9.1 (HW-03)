list1 = input()
list1 = = list(map(int, input().split()))
list1 = []
element = int(input())


def find_element(list1 = [], element = int):
    n = len(list1)
    for i in range(len(list1)):
        idx_min = i  # сохраняем индекс предположительно минимального элемента
        for j in range(i, len(list1)):
            if list1[j] < list1[idx_min]:
                idx_min = j
        if i != idx_min:  # если индекс не совпадает с минимальным, меняем
            list1[i], list1[idx_min] = list1[idx_min], list1[i]
    print(list1)


    def search_around_element(list1, element):
        for k in range(len(list1)):
            if list1[k] <= element:
                k += k
            else:
                return print(k - 1, k, list1[k - 1], list1[k])
 print(search_around_element(list1, element))
    

print(find_element([236, 52, 8, 103, 4, 96, 1], 200))
